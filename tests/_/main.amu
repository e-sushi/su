main :: () -> void {
	
	a: vec2;
	a.x = 1.2;
	a.y = 3.4;

	b: vec2;
	b.x = 5.6;
	b.y = 7.8;

	res0 := add(a,b);
	res1 := sub(a,b);
	res2 := mul(a,b);
	res3 := div(a,b);
}

add :: (a: vec2, b: vec2) -> vec2 {
	out: vec2;
	out.x = a.x + b.x;
	out.y = a.y + b.y;
	out
}

sub :: (a: vec2, b: vec2) -> vec2 {
	out: vec2;
	out.x = a.x - b.x;
	out.y = a.y - b.y;
	out
}

mul :: (a: vec2, b: vec2) -> vec2 {
	out: vec2;
	out.x = a.x * b.x;
	out.y = a.y * b.y;
	out
}

div :: (a: vec2, b: vec2) -> vec2 {
	out: vec2;
	out.x = a.x / b.x;
	out.y = a.y / b.y;
	out
}

vec2 :: struct {
	x: f32;
	y: f32;
}

